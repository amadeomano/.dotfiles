call plug#begin('~/.local/share/nvim/plugged')

  Plug 'mxw/vim-jsx'					" jsx syntax highlighting
  Plug 'pangloss/vim-javascript'			" js syntax highlighting
  Plug 'joshdick/onedark.vim'				" one-dark color scheme
  Plug 'junegunn/fzf', { 'do': { -> fzf#install() } }	" fzf fuzzy search plugin
  Plug 'junegunn/fzf.vim'				" fzf additional commands
  Plug 'neoclide/coc.nvim', {'branch': 'release'}       " coc auto-completion

call plug#end()
